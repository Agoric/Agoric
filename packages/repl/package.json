{
  "name": "@agoric/repl",
  "version": "0.1.0",
  "description": "Agoric's Javascript Read-Eval-Print-Loop",
  "parsers": {
    "js": "mjs"
  },
  "main": "src/repl.js",
  "repository": "https://github.com/Agoric/agoric-sdk",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:nyc": "nyc ava",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix && yarn lint:types",
    "lint-check": "yarn lint",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:types": "tsc -p jsconfig.json",
    "lint:eslint": "eslint '**/*.js'"
  },
  "keywords": [],
  "author": "Agoric",
  "license": "Apache-2.0",
  "dependencies": {
    "@agoric/babel-parser": "^7.6.4",
    "@agoric/eventual-send": "^0.13.10",
    "@agoric/install-ses": "^0.5.9",
    "@agoric/marshal": "^0.4.7",
    "@agoric/nat": "^4.0.0",
    "@agoric/promise-kit": "^0.2.9",
    "@agoric/store": "^0.4.10",
    "@agoric/transform-eventual-send": "^1.4.6",
    "@babel/generator": "^7.6.4",
    "esm": "^3.2.25",
    "rollup": "^1.26.2",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "devDependencies": {
    "ava": "^3.12.1",
    "nyc": "^15.1.0"
  },
  "eslintConfig": {
    "extends": [
      "@agoric"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=11.0"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "require": [
      "esm"
    ],
    "timeout": "2m"
  }
